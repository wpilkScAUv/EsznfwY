{
  "name": "Local CDN",
  "short_name": "ilclcdn",
  "description": "Local emulation of Content Delivery Networks",
  "author": "James Fray",
  "version": "0.1.3",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "homepage_url": "http://add0n.com/local-cdn.html",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  },
  "background": {
    "scripts": [
      "resources.js",
      "validate.js",
      "common.js"
    ]
  },
  "web_accessible_resources": [
    "data/resources/*"
  ],
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png",
      "64": "data/icons/64.png"
    }
  }
}
